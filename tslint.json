{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules":{
    "interface-name":false,
    "ordered-imports":false,
    "class-name":false,
    "member-access":false,
    "object-literal-sort-keys":false,
    "no-console":false,
    "no-unused-expression": false,
    "no-unused-variable": false
  }
}
